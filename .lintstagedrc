{
  "*.{js,jsx,ts,tsx}": [
    "eslint --cache --fix",
    "prettier --write --list-different",
    "git add ."
  ],
  "*.{json,md,html}": [
    "prettier --write --list-different",
    "git add ."
  ],
  "*.{css,scss,sass}": [
    "pnpm stylelint:fix",
    "prettier --write --list-different",
    "git add ."
  ]
}
