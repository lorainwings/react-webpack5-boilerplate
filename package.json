{
  "name": "react-webpack5-boilerplate",
  "version": "1.0.0",
  "description": "A webpack 5 boilerplate with React TypeScript",
  "author": "Lorain",
  "license": "MIT",
  "scripts": {
    "start": "webpack serve --config config/webpack.dev.js",
    "build": "webpack --config config/webpack.prod.js --progress",
    "prepare": "husky install",
    "eslint": "eslint src --ext .js,.jsx,.ts,.tsx --cache",
    "eslint:fix": "npm run eslint -- --fix",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "lint": "npm run eslint && npm run prettier",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix"
  },
  "keywords": [
    "webpack 5",
    "Babel 7",
    "React",
    "TypeScript"
  ],
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-react": "^7.17.12",
    "@babel/preset-typescript": "^7.18.6",
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.3.2",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "autoprefixer": "^9.8.5",
    "babel-loader": "^8.2.5",
    "commitlint": "^17.2.0",
    "cross-env": "^7.0.2",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.31.8",
    "eslint-webpack-plugin": "^3.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "image-webpack-loader": "^8.1.0",
    "lint-staged": "^13.0.3",
    "mini-css-extract-plugin": "^1.0.0",
    "postcss": "^8.4.18",
    "postcss-loader": "^7.0.1",
    "prettier": "^2.7.1",
    "sass": "^1.56.0",
    "sass-loader": "^13.1.0",
    "style-loader": "^3.3.1",
    "typescript": "^4.8.4",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4",
    "webpack-merge": "^5.8.0",
    "webpackbar": "^5.0.2"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:lorainwings/webpack5-boilerplate.git"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.19.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.39.1",
    "react-router-dom": "^6.4.3",
    "whatwg-fetch": "^3.6.2"
  }
}
